# ls
alias ll='ls -alF'
alias la='ls -A'
alias l='ls -CF'

# npm
alias nr='npm run $*'
alias ns='npm start'
alias ni='npm install --save --registry=http://registry.npm.qiwoo.org $*'

# git
alias gpull='git pull $*'
alias gpush='git push'
alias gst='git status'
alias gbr='git branch $*'
alias gch='git checkout $*'
alias gmg='git merge $*'
